{
    "manifest_version": 3,
    "version": "0.1",
    "name": "MeetingShh",
    "description": "Close all running FB chat",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/img/meeting-16.png",
            "32": "/img/meeting-32.png",
            "48": "/img/meeting-48.png",
            "128": "/img/meeting-128.png"
        }
    },
    "icons": {
        "16": "/img/meeting-t-16.png",
        "32": "/img/meeting-t-32.png",
        "48": "/img/meeting-t-48.png",
        "128": "/img/meeting-t-128.png"
    },
    // "background": {
    //     "service_worker": "background.js"
    // },
    "permissions": [
        "storage"
    ]
    
}